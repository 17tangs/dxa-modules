@model CacheEntityModel
<div class="entity" @Html.DxaEntityMarkup()>
    <h5>Rendered @Model with Entity View "Test:CacheEntityModel"</h5>
    <h4>This entity model is an example of a model without DxaNoCache attribute</h4>
    <table border="1">
        <tr>
            <td>TextField</td>
            <td @Html.DxaPropertyMarkup(() => Model.Text)>@Model.Text</td>
        </tr>
        <tr>
            <td>NumberField</td>
            <td @Html.DxaPropertyMarkup(() => Model.Number)>@Model.Number</td>
        </tr>
        <tr>
            <td>DateField</td>
            <td @Html.DxaPropertyMarkup(() => Model.Date)>@Model.Date</td>
        </tr>
    </table>
    <p>dateTime.now = @Model.DateNow</p>
</div>
